    "build:vercel": "node scripts/vercel-build.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "release": "bumpp --commit --push --tag",
    "prepare": "husky",
    "typecheck": "vue-tsc --noEmit"
