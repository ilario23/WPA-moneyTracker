{
  "name": "turtlet",
  "type": "module",
  "version": "0.8.3",
  "packageManager": "pnpm@10.17.1",
  "description": "Smart savings, one slow step at a time",
  "license": "MIT",
  "scripts": {
    "generate:build-info": "node scripts/gen-build-info.js",
